    "link_month = [] #สร้างลิสต์ขึ้นมาเก็บลิ้งในแต่ละเดือน\n",
    "for i in html_soup.find_all(\"a\",href=True): #ลูปใช้เมธอด find_all()เพื่อค้นหาทุกองค์ประกอบ \"a\" ที่มีแอตทริบิวต์ href=True\n",
    "    link = \"http://www.it.kmitl.ac.th/~teerapong/news_archive/\" + i[\"href\"] #คือสร้างตัวแปรเก็บลิ้งปกติแต่หลังจาก / ใส่ลูปของiที่เราหามาในแต่ละแท็คa ที่href=True\n",
    "    if \"month\" in link: #สร้างเงื่อนไขเพื่อเอาhrefเฉพาะที่มีสตริงชื่อmonth\n",
    "        link_month.append(link) #เพิ่มลิ้งเข้าไปในลิสต์\n",
    "link_month #โชว์ทุกลิ้งที่อยู่ในลิสต์"
